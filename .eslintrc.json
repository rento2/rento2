{
  "root": true,
  "extends": [
    "eslint:recommended"
  ],
  "overrides": [
    {"files": ["*.{jsx, ts, tsx}"]}
  ],
  "rules": {
    "@typescript-eslint/dot-notation": 0,
    "no-unused-vars": [
      "error",
      {
        "args": "none",
        "caughtErrors": "none",
        "ignoreRestSiblings": true,
        "vars": "all",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
