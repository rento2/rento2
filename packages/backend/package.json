{
  "name": "rento_backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext .ts,.js && npx tsc --noEmit",
    "lint-fix": "eslint . --ext .ts,.js --fix",
    "test": "node -r @adonisjs/assembler/build/register japaFile.ts",
    "coverage": "nyc npm run test",
    "precommit": "npm run lint",
    "dc:up": "docker-compose up -d",
    "db:reset": "node ace db:reset",
    "start:swagger": "npm run dev & start http://127.0.0.1:3333/docs/index.html & xdg-open http://127.0.0.1:3333/docs/index.html & open http://127.0.0.1:3333/docs/index.html"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.1.0",
    "@adonisjs/core": "^5.7.5",
    "@adonisjs/drive-s3": "^1.2.0",
    "@adonisjs/lucid": "^17.2.0",
    "@adonisjs/redis": "^7.2.0",
    "@adonisjs/repl": "^3.1.6",
    "adonis-lucid-soft-deletes": "^1.3.3",
    "adonis5-jwt": "^1.1.4",
    "adonis5-swagger": "^1.4.1",
    "axios": "^0.24.0",
    "build-url-ts": "^6.1.3",
    "luxon": "^2.0.2",
    "murmurhash": "^2.0.1",
    "pg": "^8.7.3",
    "phc-bcrypt": "^1.0.7",
    "proxy-addr": "^2.0.7",
    "rabbit-queue": "^5.4.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.6.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.19.0",
    "execa": "^5.1.1",
    "get-port": "^5.1.1",
    "japa": "^3.1.1",
    "nyc": "^15.1.0",
    "pino-pretty": "^7.0.0",
    "supertest": "^6.1.6",
    "typescript": "~4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  }
}
